# EcoWattch App Environment Configuration
# Copy this file to .env and fill in your actual credentials
# NEVER commit the .env file with real credentials to version control

# Willow API v3 Configuration
WILLOW_ORGANIZATION_URL=https://northernarizonauniversity.app.willowinc.com/
WILLOW_CLIENT_ID=your_client_id_here
WILLOW_CLIENT_SECRET=your_client_secret_here

# Azure Database Configuration (if applicable)
AZURE_DB_CONNECTION_STRING=your_azure_db_connection_string_here

# Other API Keys (add as needed)
# ANALYTICS_API_KEY=your_analytics_key_here
# PUSH_NOTIFICATION_KEY=your_push_notification_key_here

# App Configuration
APP_DEBUG=true
APP_VERSION=1.0.0
